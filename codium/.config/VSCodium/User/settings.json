{
    "workbench.iconTheme": "catppuccin-mocha",
    "workbench.colorTheme": "Zacks Contrast (rainglow)",
    "editor.fontFamily": "'DankMono Nerd Font', 'Droid Sans Mono', 'monospace', monospace",
    "editor.fontSize": 16,
    "editor.fontLigatures": true,
    "files.insertFinalNewline": true,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "terminal.integrated.fontFamily": "'DankMono Nerd Font', 'JetBrains Mono NFM', 'monospace', monospace",
    "terminal.integrated.fontSize": 16,
    "glassit.alpha": 220,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "explorer.compactFolders": false,
    "workbench.tree.indent": 16,
    "window.zoomLevel": 1,
    "workbench.productIconTheme": "bongocat",
    "editor.codeLens": false,
    "editor.inlineSuggest.enabled": false,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.formatOnSave": true,
    "git.autofetch": true,
}
